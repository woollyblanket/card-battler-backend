{
	"name": "backend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"test": "NODE_ENV=test c8 mocha --recursive --exit --timeout 60000",
		"test-debug": "NODE_ENV=test DEBUG=battler:* c8 mocha --recursive --exit --timeout 60000",
		"test-doppler": "NODE_ENV=test doppler run -- c8 mocha --recursive --exit --timeout 60000",
		"test-partial": "NODE_ENV=test DEBUG=battler:* doppler run -- npx wtfnode node_modules/.bin/_mocha --grep \"$npm_config_battler_flag\" test/**/*.test.js",
		"start": "node ./bin/www.js",
		"dev": "doppler run --mount .env -- nodemon ./bin/www.js",
		"koa": "doppler run --mount .env -- nodemon koa.js"
	},
	"dependencies": {
		"@koa/cors": "^4.0.0",
		"@koa/router": "^12.0.0",
		"body-parser": "^1.20.1",
		"change-case": "^4.1.2",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"csrf-sync": "^4.0.0",
		"debug": "^4.3.4",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-mongo-sanitize": "^2.2.0",
		"express-rate-limit": "^6.7.0",
		"express-response-formatter": "^2.0.2",
		"express-session": "^1.17.3",
		"express-validator": "^6.14.3",
		"glob": "^8.1.0",
		"helmet": "^6.0.1",
		"joi": "^17.8.3",
		"joi-objectid": "^4.0.2",
		"koa": "^2.14.1",
		"koa-bodyparser": "^4.3.0",
		"koa-helmet": "^6.1.0",
		"koa-logger": "^3.2.1",
		"lodash": "^4.17.21",
		"memorystore": "^1.6.7",
		"mongodb-memory-server": "^8.11.4",
		"mongoose": "^6.9.0",
		"morgan": "^1.10.0",
		"pluralize": "^8.0.0",
		"serve-favicon": "^2.5.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.20.7",
		"@babel/preset-env": "^7.20.2",
		"@babel/register": "^7.18.9",
		"babel-plugin-istanbul": "^6.1.1",
		"c8": "^7.12.0",
		"chai": "^4.3.7",
		"cross-env": "^7.0.3",
		"eslint": "^8.33.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"find-unused-exports": "^5.0.0",
		"mocha": "^10.2.0",
		"nodemon": "^2.0.20",
		"nyc": "^15.1.0",
		"supertest": "^6.3.3",
		"wtfnode": "^0.9.1"
	},
	"config": {
		"mongodbMemoryServer": {},
		"nyc": {
			"require": [
				"@babel/register"
			],
			"reporter": [
				"lcov",
				"text"
			],
			"sourceMap": false,
			"instrument": false
		}
	}
}
